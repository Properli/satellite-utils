{
  "name": "@properli/creators-lib",
  "version": "0.0.0",
  "description": "Library to encode properli watermarks into blob files",
  "main": "lib/creatorsLib.js",
  "module": "src/index.js",
  "private": true,
  "scripts": {
    "test": "ava",
    "build": "webpack --mode=development",
    "prepublish": "webpack --mode=production",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "directories": {
    "lib": "./lib"
  },
  "browser": {
    "fs": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Properli/creators-lib.git"
  },
  "keywords": [
    "copyright",
    "watermark",
    "properli",
    "sign"
  ],
  "author": "Jannis Blossey",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Properli/creators-lib/issues"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "homepage": "https://github.com/Properli/creators-lib#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.177",
    "@types/mock-fs": "^4.13.1",
    "@types/tmp": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "ava": "^3.15.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-plugin-ava": "^13.1.0",
    "eslint-plugin-import": "^2.25.2",
    "mock-fs": "^5.1.2",
    "tmp": "^0.2.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "argon2": "^0.28.2",
    "buffer": "^6.0.3",
    "lodash": "^4.17.21",
    "stream-browserify": "^3.0.0"
  }
}
